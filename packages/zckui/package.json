{
  "name": "zckui-uniapp",
  "displayName": "zckui-uniapp 智充客风格的轻量级移动端 Uniapp、Vue3 组件库",
  "version": "0.0.0",
  "description": "智充客风格的轻量级移动端 Uniapp、Vue3 组件库（支持小程序开发）",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/zckui-uniapp/zckui-uniapp#readme",
  "repository": "https://github.com/zckui-uniapp/zckui-uniapp.git",
  "bugs": "https://github.com/zckui-uniapp/zckui-uniapp/issues",
  "keywords": [
    "zckui",
    "小程序",
    "vue3",
    "mini-program",
    "vue3 component"
  ],
  "sideEffects": [
    "*.scss",
    "*/components/**/*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "components",
    "dist",
    "global.d.ts",
    "styles"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@uni-helper/uni-app-types": "^1.0.0-alpha.6",
    "vue": ">=3.2.0"
  }
}
